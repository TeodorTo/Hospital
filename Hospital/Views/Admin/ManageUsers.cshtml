@model IEnumerable<Hospital.Data.ApplicationUser>

<h1>Manage Users</h1>

<table class="table">
    <thead>
    <tr>
        <th>Username</th>
        <th>Email</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
    <tr>
        <td>@user.UserName</td>
        <td>@user.Email</td>
    </tr>
    }
    </tbody>
</table>
